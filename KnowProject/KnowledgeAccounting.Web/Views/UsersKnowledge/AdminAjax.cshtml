@{
    List<SelectListItem> list = new List<SelectListItem>();

    list.Add(new SelectListItem() { Text = "Find" });
    list.Add(new SelectListItem() { Text = "MakeDoc" });
}


<div class="nav navbar-nav navbar-left" >
    @using (Html.BeginForm("Find", "Users", null))
    {
        @Html.AntiForgeryToken()

        <div class="form-inline">
            <div class="form-group">
                @Html.Label("Knowledges")
                <div class="col-md-10">
                    @Html.DropDownList("Knowledges", ViewBag.Knowledges as SelectList, new { htmlAttributes = new { @class = "form-control" } })
                </div>
            </div>
            <div class="form-group">
                @Html.Label("Degree")
                <div class="col-md-10">
                    @Html.DropDownList("Numbers", ViewBag.Numbers as SelectList, new { htmlAttributes = new { @class = "form-control" } })
                </div>
            </div>
            <div class="form-group">
                @Html.Label("WhatDoYouWanna")
                <div class="col-md-10">
                    @Html.DropDownList("WhatDoYouWanna", list)
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Make" class="btn btn-default" />
                </div>
            </div>

        </div>

    }
</div>



